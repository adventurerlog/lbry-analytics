{
  "name": "lbry-analytics",
  "version": "0.0.0",
  "description": "A simple starter application that bundles the lbry sdk",
  "scripts": {
    "start": "electron ./src/main",
    "dev": "electron ./src/main/index.js",
    "postinstall": "node build/download-sdk.js && electron-builder install-app-deps",
    "clear": "",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "main": "./src/main/index.js",
  "build": {
    "appId": "lbry.analytics",
    "win": {
      "target": [
        "portable"
      ]
    },
    "portable": {
      "artifactName": "lbry_analytics.exe"
    },
    "directories": {
      "output": "pack",
      "app": "./",
      "buildResources": "pack"
    }
  },
  "dependencies": {
    "find-process": "^1.2.0",
    "highlight.js": "^9.18.1",
    "lbry-redux": "lbryio/lbry-redux",
    "node-fetch": "^2.6.1",
    "url": "^0.11.0",
    "vue": "^1.0.28"
  },
  "devDependencies": {
    "decompress": "^4.2.1",
    "del": "^5.1.0",
    "electron": "^7.1.11",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.2.5"
  },
  "lbrySettings": {
    "lbrynetSDKVersion": "0.93.0",
    "lbrynetSDKUrlTemplate": "https://github.com/lbryio/lbry/releases/download/vSDKVER/lbrynet-OSNAME.zip",
    "lbrynetSDKDir": "dist/sdk",
    "lbrynetSDKFileName": "lbrynet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/adam-dorin/lbry-analytics.git"
  },
  "author": "https://gitlab.com/adam-dorin/lbry-analytics/-/project_members",
  "license": "MIT",
  "homepage": "https://gitlab.com/adam-dorin/lbry-analytics"
}
